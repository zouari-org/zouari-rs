{
  "name": "zouari-rs",
  "version": "3.2.1",
  "description": "A modern, high-performance, and secure stack built on Rust.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "sync:schemas": "curl -o packages/validation/openapi.json http://localhost:5150/api-docs/openapi.json && pnpm --filter @zouari-rs/validation generate:schemas",
    "changeset": "changeset",
    "bump": "changeset version && pnpm install --no-frozen-lockfile",
    "release": "turbo run build --filter=packages/*"
  },
  "keywords": [],
