{
  "name": "zouari-rs",
  "version": "3.2.0",
  "description": "A modern, high-performance, and secure stack built on Rust.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
